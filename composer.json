{
    "name": "ubercrawler/ubercrawler",
    "description": "Uber Data Crawler and Report Generator",
    "authors": [
        {
            "name": "Bassem Dghaidi",
            "email": "bassem@interop.link"
        }
    ],
    "require": {
        "symfony/console": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "mikey179/vfsStream": "1.6.*",
        "satooshi/php-coveralls": "dev-master",
        "squizlabs/php_codesniffer": "2.*"
    },
    "config": {
        "vendor-dir": "./src/vendor"
    },
    "autoload": {
        "psr-0": {
            "tests": ""
        },
        "psr-4": {
            "UberCrawler\\": "./src"
        }
    }
}